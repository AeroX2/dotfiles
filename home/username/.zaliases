alias mv='mv -i'
alias cp='cp -i'
alias cp_p='rsync -ah --progress'
alias rm='safe-rm'

alias c='clear'
alias e='exit'

alias pup='sudo pacman -Sy'
alias pug='sudo pacman -Syu'
alias pin='sudo pacman -S '
alias pre='sudo pacman -R '
alias par='sudo pacman -Rns $(pacman -Qtdq)'

alias vi='vim'
alias python='python3'
alias resetdmenu='rm ~/.cache/dmenu_run'
alias feh='feh --scale-down '
alias nmap='sudo nmap '
alias top='htop'

alias yaourt='yaourt --noconfirm'
